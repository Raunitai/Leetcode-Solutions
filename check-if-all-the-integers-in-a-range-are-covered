#include <vector>
using namespace std;

class Solution {
public:
    bool isCovered(vector<vector<int>>& ranges, int left, int right) {
        vector<int> cover(52, 0); // up to 50, but we use end+1 so need 51

        for (auto &r : ranges) {
            cover[r[0]]++;
            cover[r[1] + 1]--;
        }

        int curr = 0;
        for (int i = 1; i <= 50; i++) {
            curr += cover[i];
            if (i >= left && i <= right && curr <= 0) {
                return false;
            }
        }

        return true;
    }
};
