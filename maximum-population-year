#include <vector>
using namespace std;

class Solution {
public:
    int maximumPopulation(vector<vector<int>>& logs) {
        vector<int> years(2051, 0); // years from 1950 to 2050

        for (auto &log : logs) {
            years[log[0]]++;   // birth
            years[log[1]]--;   // death (not included)
        }

        int maxPop = 0, year = 1950, current = 0;
        for (int i = 1950; i <= 2050; i++) {
            current += years[i];
            if (current > maxPop) {
                maxPop = current;
                year = i;
            }
        }

        return year;
    }
};
